@article{TAcomposition,
abstract = {Telescope Array (TA) has recently published results of nearly nine years of {\$}X{\_}{\{}\backslashmathrm{\{}max{\}}{\}}{\$} observations providing its highest statistics measurement of ultra high energy cosmic ray (UHECR) mass composition to date for energies exceeding {\$}10{\^{}}{\{}18.2{\}}{\$} eV. This analysis measured agreement of observed data with results expected for four different single elements. Instead of relying only on the first and second moments of {\$}X{\_}{\{}\backslashmathrm{\{}max{\}}{\}}{\$} distributions, we employ a morphological test of agreement between data and Monte Carlo to allow for systematic uncertainties in data and in current UHECR hadronic models. Results of this latest analysis and implications of UHECR composition observed by TA are presented. TA can utilize different analysis methods to understand composition as both a crosscheck on results and as a tool to understand systematics affecting {\$}X{\_}{\{}\backslashmathrm{\{}max{\}}{\}}{\$} measurements. The different analysis efforts underway at TA to understand composition are also discussed.},
archivePrefix = {arXiv},
arxivId = {1812.05688},
author = {Hanlon, William},
eprint = {1812.05688},
file = {:home/cindy/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Hanlon - Unknown - Measurements of UHECR Mass Composition by Telescope Array.pdf:pdf},
title = {{Measurements of UHECR Mass Composition by Telescope Array}},
url = {https://arxiv.org/pdf/1812.05688.pdf http://arxiv.org/abs/1812.05688},
year = {2018}
}

@article{PAOcomposition,
abstract = {We present a combined fit of a simple astrophysical model of UHECR sources to both the energy spectrum and mass composition data measured by the Pierre Auger Observatory. The fit has been performed for energies above {\$}5 \backslashcdot 10{\^{}}{\{}18{\}}{\$} eV, i.e.{\~{}}the region of the all-particle spectrum above the so-called "ankle" feature. The astrophysical model we adopted consists of identical sources uniformly distributed in a comoving volume, where nuclei are accelerated through a rigidity-dependent mechanism. The fit results suggest sources characterized by relatively low maximum injection energies, hard spectra and heavy chemical composition. We also show that uncertainties about physical quantities relevant to UHECR propagation and shower development have a non-negligible impact on the fit results.},
archivePrefix = {arXiv},
arxivId = {1612.07155v3},
author = {Pierre Auger Collaboration},
doi = {10.1088/1475-7516/2017/04/038},
eprint = {1612.07155v3},
