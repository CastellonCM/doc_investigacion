file = {:home/cindy/Desktop/Mendeley/Nagano et al. - 2000 - Comparison of AGASA data with CORSIKA simulation.pdf:pdf},
issn = {09276505},
journal = {Astroparticle Physics},
keywords = {96.40.De,96.40.Pq,Cosmic ray,Extensive air shower,Primary energy estimation,Simulation},
number = {4},
pages = {277--294},
title = {{Comparison of AGASA data with CORSIKA simulation}},
volume = {13},
year = {2000}
}

